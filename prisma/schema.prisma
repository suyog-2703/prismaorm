generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users{
  id Int @id @default(autoincrement())
  first_name String @db.VarChar(192)
  last_name String @db.VarChar(192)
  email String @unique @db.VarChar(192)
  password String
}
